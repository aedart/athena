{
  "name": "@aedart/contracts",
  "version": "0.0.0",
  "description": "Athena contracts package",
  "author": "Alin Eugen Deac <aedart@gmail.com>",
  "homepage": "https://github.com/aedart/athena/tree/main/packages/alpha#readme",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/aedart/athena/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aedart/athena.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/contracts.cjs.js",
  "module": "dist/contracts.esm.js",
  "browser": "dist/contracts.umd.js",
  "types": "dist/contracts.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/contracts.cjs.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "contracts": [ "dist/contracts.d.ts" ],
      "container": [ "dist/container.d.ts" ],
      "meta": [ "dist/meta.d.ts" ]
    }
  },
  "scripts": {
    "compile": "rollup -c",
    "watch": "rollup -c -w"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  }
}
